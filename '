import json

class Movie(object):
    def __init__(self, title, id_number):
        self.title = title
        self.id = id_number

    def __str__(self):
        return self.title



def jsonToMovie(inString):
    decoded = json.loads(inString)
    movies = decoded['movies']
    print len(movies)
    for movie in movies:
        title = movie['title']
        id_number = movie['id']
        print Movie(title, id_number)

def main():
    instring = '{"movies": [{"id": 0, "title": "tucker"}, {"id": 1, "title": "nemo"}]}'
    test = jsonToMovie(instring)
    print test

if __name__ == "__main__":
    main()

